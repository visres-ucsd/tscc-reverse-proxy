Host TSCC:
  hostname: tscc*
  endpoint: tscc-user-content.sdsc.edu
  system: slurm
  default-partition: interactive
  batch-dir: slurm
Host TSCC-GPU:
  hostname: tscc-gpu*
  endpoint: tscc-user-content.sdsc.edu
  system: slurm
  default-partition: interactive
  batch-dir: slurm
Host TSCC-Stratus:
  hostname: tscc-stratus
  endpoint: tscc-user-content.sdsc.edu
  system: slurm
  default-partition: interactive
  batch-dir: slurm
Host Comet:
  hostname: comet-*
  endpoint: comet-user-content.sdsc.edu
  system: slurm
  default-partition: compute
  batch-dir: slurm
